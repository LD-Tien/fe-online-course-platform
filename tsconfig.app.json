{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "auto-imports.d.ts", "src/vuex.d.ts"],
    "exclude": ["src/**/__tests__/*", "node_modules", "dist", "lib"],
    "compilerOptions": {
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "types": ["element-plus/global"],
        "baseUrl": ".",
        "allowJs": true,
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}
